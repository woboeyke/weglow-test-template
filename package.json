    "mapbox-gl": "1.13.2",
    "pinia": "^2.0.11",
    "quasar": "^2.6.0",
    "v-mapbox": "3.1.6",
    "vue": "^3.0.0",
    "vue-i18n": "^9.2.2",
    "vue-router": "^4.0.0",
